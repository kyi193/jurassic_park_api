<p id="notice"><%= notice %></p>

<h1>Diet Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diet_types.each do |diet_type| %>
      <tr>
        <td><%= diet_type.name %></td>
        <td><%= link_to 'Show', diet_type %></td>
        <td><%= link_to 'Edit', edit_diet_type_path(diet_type) %></td>
        <td><%= link_to 'Destroy', diet_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diet Type', new_diet_type_path %>
